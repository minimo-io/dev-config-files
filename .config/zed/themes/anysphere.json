{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "Anysphere Dark",
    "author": "Your Name",
    "themes": [
        {
            "name": "Anysphere Dark",
            "appearance": "dark",
            "style": {
                "background.appearance": "opaque",
                "border": "#242424",
                "background": "#1e1e1e",
                "text": "#d6d6dd",
                "editor.foreground": "#d6d6dd",
                "editor.background": "#1e1e1e",
                "editor.line_number": "#878787",
                "editor.active_line_number": "#d6d6dd",
                "editor.gutter.background": "#1e1e1e"
            },
            "semanticTokenColors": {
                "enumMember": { "color": "#d6d6dd" },
                "variable.constant": { "color": "#83d6c5" },
                "variable.defaultLibrary": { "color": "#d6d6dd" },
                "variable.defaultLibrary.globalScope": { "color": "#d1d1d1" },
                "class.typeHint": { "color": "#82d2ce" },
                "variable.declaration.readonly:cpp": { "color": "#d1d1d1" },
                "variable.declaration.readonly:c": { "color": "#d1d1d1" },
                "variable.readonly:cpp": { "color": "#d1d1d1" },
                "variable.readonly:c": { "color": "#d1d1d1" },
                "variable.other.property.ts": { "color": "#AA9BF5" },
                "variable.other.property": { "color": "#AA9BF5" },

                "function.declaration": { "color": "#efb080" },
                "function.builtin": { "color": "#82d2ce" },
                "entity.name.function": { "color": "#E3C893" }
            }
        }
    ]
}
